<Page x:Class="Launcher.WPF.PageLogin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Launcher.WPF"
      mc:Ignorable="d"
      Title="PageLogin">
    <StackPanel>
        <Label x:Name="titleLabel" Content="Title" FontWeight="Bold" HorizontalContentAlignment="Center"/>
        <Button Content="Login" Width="60" Height="30" Margin="0,10,0,40" Click="ButtonLogin_Click"/>
    </StackPanel>
</Page>
