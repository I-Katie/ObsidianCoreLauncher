<Page x:Class="Launcher.WPF.PageWait"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Launcher.WPF"
      mc:Ignorable="d" 
      Title="PageWait">
    <StackPanel>
        <Label x:Name="titleLabel" Content="Title" HorizontalAlignment="Center" HorizontalContentAlignment="Center"/>
        <ProgressBar HorizontalAlignment="Center" Height="15" Width="330" Margin="0, 10, 0, 30" IsIndeterminate="true"/>
    </StackPanel>
</Page>
